<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyecto.HelloController">

    <HBox spacing="10.0" style="-fx-background-color: #7ddf64; -fx-padding: 10;">
        <Button fx:id="btnBackToMenu" onAction="#handleBackToMenu" style="-fx-font-size: 14px;" text="← Volver al Menú" />
    </HBox>

    <WebView fx:id="svgView" VBox.vgrow="ALWAYS" />
</VBox>
